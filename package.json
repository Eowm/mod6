{
  "name": "learning-git-2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "watch": "npm-run-all -p watch:browsersync watch:sassprefixer",
    "watch:browsersync":"browser-sync start --server --files css/*.css *.html",
    "watch:sass": "node-sass --output-style expanded --source-map true -o ./css/ ./scss/style.scss --watch",
    "watch:autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.prefixed.css css/style.css",
    "watch:sassprefixer": "npm run watch:sass && npm run watch:autoprefixer",
    "build": "npm run build:clean && npm run build:sass",
    "build:clean": "npm rm -rf https://eowm.github.io/learning-git-2/css/",
    "build:sass": "node-sass --watch --recursive --output css --source-map true --source-map-contents sass"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "https://eowm.github.io/learning-git-2/",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.24.4",
    "globstar": "^1.0.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "nu-html-checker": "^0.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eowm/learning-git-2.git"
  },
  "bugs": {
    "url": "https://github.com/Eowm/learning-git-2/issues"
  },
  "homepage": "https://github.com/Eowm/learning-git-2#readme",
  "dependencies": {
    "onchange": "^4.0.0"
  }
}
